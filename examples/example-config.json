{
  "include-private": true,
  "show-skipped": false,
  "show-version": false,
  "interval": 30,
  "refresh-rate": 250,
  "log-dir": "logs",
  "log-file": "autogitpull.log",
  "concurrency": 4,
  "check-only": false,
  "no-hash-check": false,
  "log-level": "INFO",
  "verbose": false,
  "max-threads": 8,
  "cpu-percent": 50,
  "cpu-cores": "0xff",
  "mem-limit": 0,
  "no-cpu-tracker": false,
  "no-mem-tracker": false,
  "no-thread-tracker": false,
  "threads": 4,
  "single-thread": false,
  "net-tracker": false,
  "download-limit": 0,
  "upload-limit": 0,
  "disk-limit": 0,
  "cli": false,
  "silent": false,
  "recursive": false,
  "max-depth": 0,
  "force-pull": false,
  "discard-dirty": false,
  "single-run": false,
  "cpu-poll": 5,
  "mem-poll": 5,
  "thread-poll": 5
}
